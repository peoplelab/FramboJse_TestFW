<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Main.pvt.Master.cs" Inherits="FramboJSe_Test.MasterPages.Main_pvt" %>

<!DOCTYPE html>

<html>

	<head runat="server">
	
	<title>FramboJSe_Test - Public</title>

    <!-- Required meta tags -->
